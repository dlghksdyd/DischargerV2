<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Mex="clr-namespace:MExpress.Mex">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/MExpress;component/StaticResources/ResColor.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

    <Mex:ColorSet_ComboBoxItem x:Key="Mex/ResColorSet_ComboBoxItem/Primary"
                               BackgroundDefault="{StaticResource Mex/ResColor/surface_primary}"
                               BackgroundHover="{StaticResource Mex/ResColor/table_action_hover}"
                               BackgroundSelected="{StaticResource Mex/ResColor/table_selected}"
                               BackgroundDisabled="{StaticResource Mex/ResColor/surface_disabled}"
                               
                               ForegroundDefault="{StaticResource Mex/ResColor/text_body}"
                               ForegroundHover="{StaticResource Mex/ResColor/text_body}"
                               ForegroundSelected="{StaticResource Mex/ResColor/text_headings}"
                               ForegroundDisabled="{StaticResource Mex/ResColor/text_on_disabled}">
    </Mex:ColorSet_ComboBoxItem>

</ResourceDictionary>