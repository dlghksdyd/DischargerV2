<Window
    x:Class="DischargerV2.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Mex="clr-namespace:MExpress.Mex;assembly=MExpress"
    xmlns:Util="clr-namespace:Utility.Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DischargerV2"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:view="clr-namespace:DischargerV2.MVVM.Views"
    Title="MainWindow"
    Width="500"
    Height="400"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Grid>
        <Image Source="{StaticResource Mex/ResImage/bg_login}" Stretch="Fill" />
    </Grid>
</Window>