<Application x:Class="DischargerV2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DischargerV2"
             StartupUri="MainWindow.xaml"
             xmlns:viewModel="clr-namespace:DischargerV2.MVVM.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <viewModel:DischargerViewModel x:Key="DischargerViewModel"></viewModel:DischargerViewModel>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MExpress;component/Themes/Generic.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
