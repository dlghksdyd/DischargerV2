<Application x:Class="DischargerV2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DischargerV2"
             xmlns:vm="clr-namespace:DischargerV2.MVVM.ViewModels"
             xmlns:str="clr-namespace:DischargerV2.Languages.Strings"
             StartupUri="/MVVM/Views/ViewMain.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelMain x:Key="ViewModelMain"/>
            <vm:ViewModelLogin x:Key="ViewModelLogin"/>

            <str:DynamicString x:Key="DynamicString"/>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MExpress;component/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
