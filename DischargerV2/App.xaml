<Application x:Class="DischargerV2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DischargerV2"
             StartupUri="MainWindow.xaml"
             xmlns:vm="clr-namespace:DischargerV2.MVVM.ViewModels"
             xmlns:converter="clr-namespace:DischargerV2.MVVM.Converter">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLogin x:Key="ViewModelLogin"></vm:ViewModelLogin>
            <vm:ViewModelDischarger x:Key="DischargerViewModel"></vm:ViewModelDischarger>

            <converter:StartDischargerParameterConverter x:Key="StartDischargerParameterConverter"></converter:StartDischargerParameterConverter>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MExpress;component/Themes/Generic.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
